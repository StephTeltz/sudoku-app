    "url": "https://github.com/StephTeltz/sudoku/issues"
  },
  "homepage": "https://github.com/StephTeltz/sudoku#readme",
  "dependencies": {
    "@babel/runtime": "^7.11.2",
    "body-parser": "^1.19.0",
    "core-js": "^3.6.5",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "node-fetch": "^2.6.1",
    "react": "16.8.4",
    "react-dom": "^16.8.4",
    "react-redux": "^7.2.1",
    "react-scripts": "3.4.3",
    "redux": "^4.0.5",
    "regenerator-runtime": "^0.13.7"
  },
